    "youtube-comment-api": "^3.0.1"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-stream": "0.0.0",
    "eslint": "^4.1.1",
    "husky": "^0.14.1",
    "mocha": "^3.4.2",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  },
  "tonicExampleFilename": "runkit-example.js"
